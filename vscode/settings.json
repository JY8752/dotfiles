{
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[elm]": {
    "tailwindCSS.experimental.classRegex": ["\\bclass\\s+\"([^\"]*)\""]
  },
  "[javascript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust-analyzer"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
    // "editor.defaultFormatter": "biomejs.biome"
  },
  "background.interval": 600,
  "code-runner.executorMap": {
    "python": "python3 -u",
    "rust": "cargo run # $fileName"
  },
  "code-runner.runInTerminal": true,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "quickfix.biome": "explicit",
    "source.organizeImports.biome": "explicit"
  },
  "editor.inlineSuggest.enabled": true,
  "editor.minimap.enabled": false,
  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.tabSize": 2,
  "extensions.experimental.affinity": {
    "asvetliakov.vscode-neovim": 1
  },
  "files.associations": {
    "*.tmpl": "html"
  },
  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "git.openRepositoryInParentFolders": "always",
  "github.copilot.enable": {
    "*": true,
    "markdown": "true",
    "plaintext": false,
    "scminput": false
  },
  "go.toolsManagement.autoUpdate": true,
  "protoc": {
    "editor.formatOnSave": true,
    "options": ["-I=~/.cache/buf/v1/module/data/buf.build"]
  },
  "redhat.telemetry.enabled": true,
  "rust-analyzer.check.command": "clippy",
  "shellcheck.ignorePatterns": {
    "**/.envrc": true
  },
  "solidity.compileUsingRemoteVersion": "v0.8.15+commit.e14f2714",
  "solidity.defaultCompiler": "remote",
  "solidity.packageDefaultDependenciesContractsDirectory": "src",
  "solidity.packageDefaultDependenciesDirectory": "lib",
  "tailwindCSS.experimental.classRegex": [
    "tw\\('([^')]*)",
    "classnames\\(([^)]*)\\)",
    "'([^']*)'"
  ],
  "tailwindCSS.includeLanguages": {
    "elm": "html"
  },
  "terminal.explorerKind": "external",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.env.osx": {
    "CW_NEW_SESSION": "1",
    "Q_NEW_SESSION": "1"
  },
  "typescript.preferences.importModuleSpecifier": "relative",
  "vim.hlsearch": true,
  "vim.useSystemClipboard": true,
  "vim.insertModeKeyBindings": [
    {
      "after": ["<Esc>"],
      "before": ["j", "j"]
    },
    {
      "after": ["<Esc>"],
      "before": ["„Å£", "j"]
    },
    {
      "before": ["<C-g>", "<C-u>"],
      "commands": ["<Esc>", "gUiw", "gi"]
    },
    {
        "before": ["<C-g>", "<C-l>"],
        "commands": ["<Esc>", "guiw", "gi"]
    },
    {
        "before": ["<C-g>", "<C-k>"],
        "commands": ["<Esc>", "b", "gU", "l", "gi"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["u"],
      "commands": ["undo"]
    },
    {
      "before": ["U"],
      "commands": ["redo"]
    },

  ],
  "vs-kubernetes": {
    "vscode-kubernetes.minikube-path-mac": "/Users/yamanakajunichi/.vs-kubernetes/tools/minikube/darwin-arm64/minikube",
    "vscode-kubernetes.minikube-path.mac": "/Users/yamanakajunichi/.vs-kubernetes/tools/minikube/darwin-arm64/minikube"
  },
  "workbench.colorTheme": "Iceberg",
  "workbench.iconTheme": "catppuccin-mocha",
  "workbench.startupEditor": "none",
  "github.copilot.editor.enableAutoCompletions": true,
  "cSpell.userWords": [
    "runn",
    "zenn"
  ],
  "vim.vimrc.enable": true,
  "workbench.activityBar.location": "top",
  "editor.inlineSuggest.suppressSuggestions": true,
  "haskell.manageHLS": "GHCup",
}
